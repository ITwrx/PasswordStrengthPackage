{
    "name": "itwrx/password-strength",
    "description": "This package provides a validator for ensuring strong passwords in Laravel 6 applications.",
    "keywords": [
        "laravel",
        "laravel6",
        "laravel 6",
        "validation",
        "password",
        "password strength"
    ],
    "authors": [
        {
            "name": "Oliver Schupp",
            "email": "oliver.schupp@yahoo.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~6.0",
        "illuminate/translation": "^6.1"
    },
    "require-dev": {
        "illuminate/validation": "~6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-0": {
            "Schuppo\\PasswordStrength": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schuppo\\PasswordStrength\\PasswordStrengthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
